{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "seoSections": {
      "type": "component",
      "repeatable": true,
      "component": "blocks.seo-sections"
    },
    "seoInterlinking": {
      "type": "component",
      "repeatable": true,
      "component": "blocks.seo-interlinking"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "child",
        "parent"
      ],
      "default": "child"
    },
    "parentSlug": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
